{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./src",
    "paths": {
      "auth/*": ["auth/*"],
      "common/*": ["common/*"],
      "db/*": ["db/*"],
      "exercise-types/*": ["exercise-types/*"],
      "exercises/*": ["exercises/*"],
      "medias/*": ["medias/*"],
      "muscles/*": ["muscles/*"],
      "storage/*": ["storage/*"],
      "users/*": ["users/*"]
    },
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false
  }
}
